{
  "compilerOptions": {
    "target": "ES2022",
     // "target": "es6",
    //"target": "ESNext",
    "module": "commonjs",
   // "module": "ESNext",
    /* Specify what module code is generated. */
    "moduleResolution": "Node",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
